<mvc:View
	controllerName="com.cnc.tng.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	core:require="{
		formatter: 'com/cnc/tng/model/formatter'
	}">

	<Page
		title="{i18n>appTitle}"
		id="page">
		<content>
		<l:HorizontalLayout class="sapUiContentPadding" content="{routes>/}">

		<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{routes>title}"
		subheader="{routes>name}" press="onRoute(${routes>name})">	
		</GenericTile>

		</l:HorizontalLayout>
		</content>
	</Page>

</mvc:View>
